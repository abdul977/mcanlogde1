# MCAN Mobile App Environment Variables

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://mcanlogde1.onrender.com
EXPO_PUBLIC_API_TIMEOUT=10000

# Development Configuration
EXPO_PUBLIC_DEV_API_BASE_URL=http://localhost:3000
EXPO_PUBLIC_ENABLE_FLIPPER=false

# App Configuration
EXPO_PUBLIC_APP_NAME=MCAN
EXPO_PUBLIC_APP_VERSION=1.0.0

# Feature Flags
EXPO_PUBLIC_ENABLE_BIOMETRIC_AUTH=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true

# External Services
EXPO_PUBLIC_PRAYER_TIMES_API_KEY=your_prayer_times_api_key_here
EXPO_PUBLIC_MAPS_API_KEY=your_maps_api_key_here

# Analytics (optional)
EXPO_PUBLIC_ANALYTICS_ENABLED=false
EXPO_PUBLIC_CRASHLYTICS_ENABLED=false

# Debug Configuration
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_LOG_LEVEL=info
